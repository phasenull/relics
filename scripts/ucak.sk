command /ucak:
	trigger:
		execute player command "/v on"
		loop 9999 times:
			if {play.%player%} is true:
				execute player command "/summon ArmorStand ~1 ~-1.2 ~ {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~0.5 ~-1.1 ~ {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~0 ~-1.1 ~ {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~-0.5 ~-1.1 ~ {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~-1 ~-1.1 ~ {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~-0 ~-1.1 ~0.3 {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~-0.3 ~-1.1 ~0.6 {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~-0.6 ~-1.1 ~0.9 {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~-0.6 ~-1.1 ~0.6 {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~ ~-0.6 ~ {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:ice,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~-2 ~-1.1 ~ {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~-1.6 ~-1.1 ~ {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:42,Count:1,Damage:14}]}"
				execute player command "/summon ArmorStand ~-0.6 ~-0.6 ~ {Invisible:1,NoGravity:1,Equipment:[{},{},{},{},{id:ice,Count:1,Damage:14}]}"

				wait 0.01 seconds
				execute player command "/killall ArmorStand"
				wait 0.05 seconds
				execute player command "/sohbetimitemizle"
				execute player command "/ucakk"
			else if {stop.%player%} is true:
				cancel event
				send "&6Ucus &ckapali"

