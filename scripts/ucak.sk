command /ucakac:
	trigger:
		set {play.%player%} to true
		delete {stop.%player%}
		send "&6Ucus &6acildi"
command /ucakrl:
	trigger:
		execute player command "sk reload ucak"
		execute player command "ucakkapa"
		execute player command "ucakac"
		execute player command "ucak"